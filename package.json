{
  "name": "secman-desktop",
  "version": "1.0.3",
  "author": {
    "name": "Secman, Inc.",
    "email": "contact@secman.dev",
    "url": "https://secman.dev"
  },
  "description": "üñ•Ô∏è Secman Desktop is the desktop client for secman.",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "npm run build:clean && node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "build:darwin": "yarn build:dir && electron-packager . --overwrite --asar --platform=darwin --arch=x64 --icon=res/icon.icns --prune=true --out=build --version-string.CompanyName=secman-desktop --version-string.FileDescription=secman-desktop",
    "build:linux": "yarn build:dir && electron-packager . --overwrite --asar --platform=linux --arch=x64 --icon=res/512x512.png --prune=true --out=build --version-string.CompanyName=secman-desktop --version-string.FileDescription=secman-desktop",
    "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
    "build:win": "yarn build:dir && electron-packager . --overwrite --asar --platform=win32 --arch=x64 --icon=res/icon.ico --prune=true --out=build --version-string.CompanyName=secman-desktop --version-string.FileDescription=secman-desktop",
    "dev": "node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:linux": "electron-packager . secman-desktop --platform linux --arch x64 --out build",
    "installer:deb": "npx electron-installer-debian --src build/secman-desktop-linux-x64 --arch amd64 --dest installers/ --icon res/512x512.png",
    "installer:rpm": "npx electron-installer-redhat --src build/secman-desktop-linux-x64 --arch amd64 --dest installers/ --icon res/512x512.png",
    "installer:win": "electron-installer-windows --src build/secman-desktop-win32-x64 --arch amd64 --dest installers/ --icon res/icon.ico",
    "installer:darwin": "electron-installer-dmg ./build/secman-desktop-darwin-x64/secman-desktop.app 'Secman Desktop' --out installers/ --icon res/icon.icns",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "Secman Desktop",
    "appId": "dev.secman.desktop",
    "afterPack": "./scripts/afterPackHook.js",
    "afterSign": "electron-builder-notarize",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "res/icon.icns"
    },
    "win": {
      "icon": "res/icon.ico"
    },
    "linux": {
      "icon": "res/icon.png",
      "target": [
        "deb"
      ],
      "category": "Development"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "dotenv": "^10.0.0",
    "papaparse": "^5.2.0",
    "powershell": "^2.3.2",
    "shelljs": "^0.8.4",
    "v-tooltip": "^2.0.3",
    "vee-validate": "2.2.15",
    "vue": "^2.5.16",
    "vue-clipboard2": "^0.3.1",
    "vue-debounce": "^2.5.4",
    "vue-devtools": "^5.1.4",
    "vue-i18n": "^8.26.5",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.0.1",
    "vue-snotify": "^3.2.1",
    "vue-wait": "^1.5.3",
    "vue2-perfect-scrollbar": "^1.5.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.10.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^6.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^11.4.1",
    "electron-builder": "22.13.1",
    "electron-builder-notarize": "^1.2.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-installer-windows": "^3.0.0",
    "electron-notarize": "^1.1.1",
    "electron-packager": "^15.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.25.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^9.1.2",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^2.0.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
